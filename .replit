modules = ["nodejs-20", "web"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev"

[[workflows.workflow]]
name = "dev"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3002

[[ports]]
localPort = 34183
externalPort = 3000

[[ports]]
localPort = 43009
externalPort = 4200

[[ports]]
localPort = 44001
externalPort = 5000

[[ports]]
localPort = 44141
externalPort = 3003

[[ports]]
localPort = 45773
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "vite", "preview", "--host", "0.0.0.0", "--port", "5000"]
build = ["npm", "run", "build"]
